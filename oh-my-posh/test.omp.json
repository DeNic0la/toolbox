{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": false,
  "version": 3,
  "palette": {
    "first.bg": "#5b0269",
    "first.fg": "#FFFFFF",
    "ng.bg": "#1976d2",
    "ng.fg": "#FFFFFF",
    "second.bg": "#242424",
    "second.fg": "#f1184c",
    "docker.bg": "#0B59E7",
    "docker.fg": "#000000",
    "kubectl.bg": "#326CE5",
    "kubectl.fg": "#FFFFFF",
    "mvn.bg": "#2E2A65",
    "mvn.fg": "#FFFFFF",
    "npm.bg": "#ffeb3b",
    "npm.fg": "#193549",
    "nx.bg": "#1976d2",
    "nx.fg": "#000000",
    "pnpm.bg": "#FCA17D",
    "pnpm.fg": "#000000",
    "java.bg": "#F7DF1E",
    "java.fg": "#000000",
    "node.bg": "#8CC84B",
    "node.fg": "#000000",
    "php.bg": "#777BB3",
    "php.fg": "#FFFFFF",
    "python.bg": "#906cff",
    "python.fg": "#100e23",
    "spotify.bg": "#1DB954",
    "spotify.fg": "#FFFFFF",
    "ytm.bg": "#FF0000",
    "ytm.fg": "#FFFFFF",
    "git.bg": "#5e4800",
    "git.fg": "#ffffff",
    "git-changes.bg": "#f0ad4e",
    "git-ahead-and-behind.bg": "#5bc0de",
    "git-ahead.bg": "#000361",
    "git-behind.bg": "#d9534f",
    "execution-time.bg": "#8800dd",
    "execution-time.fg": "#ffffff",
    "os.bg": "#242424",
    "os.fg": "#ffffff",
    "folder.bg": "#26002b",
    "folder.fg": "#ffffff",
    "shell.bg": "#26002b",
    "shell.fg": "#ffffff"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "shell",
          "style": "diamond",
          "leading_diamond": "\ue0c5",
          "foreground": "p:first.fg",
          "background": "p:first.bg",
          "properties": {
            "mapped_shell_names": {
              "pwsh": "\ue86c",
              "powershell": "\ue86c",
              "ps": "\ue86c",
              "bash": "\ue760",
              "zsh": "\ue691",
              "fish": "\uee41"
            }
          }
        },
        {
          "background": "p:first.bg",
          "foreground": "p:first.fg",
          "style": "powerline",
          "powerline_symbol": "\uE0CD",
          "template": "{{if .Root }}\ueb46{{ end }}{{ if .SSHSession }}\uEBA9 {{ end }}{{ .UserName }}{{ if .SSHSession }}@{{.HostName}}{{ end }} ",
          "type": "session"
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\uE0CD",
          "foreground": "p:folder.fg",
          "background": "p:folder.bg",
          "display_root": true,
          "properties": {
            "style": "agnoster",
            "mapped_locations": {
              "C:\\temp": ""
            }
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:git.fg",
          "background": "p:git.bg",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-changes.bg{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-ahead-and-behind.bg{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-ahead.bg{{ end }}",
            "{{ if gt .Behind 0 }}p:git-behind.bg{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }}",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "source": "cli",
            "mapped_branches": {
              "feat/*": "🚀 ",
              "bug/*": "🐛 ",
              "master": "\uDB80\uDDD7",
              "main": "\ue5fd",
              "develop": "\uf339"
            }
          }
        },
        {
          "type": "angular",
          "style": "powerline",
          "cache_duration": "1h",
          "display_mode": "files",
          "powerline_symbol": "",
          "foreground": "p:ng.fg",
          "background": "p:ng.bg",
          "template": "  {{ .Full }} "
        },
        {
          "type": "argocd",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:second.fg",
          "background": "p:second.bg",
          "template": " \ue734 {{ .Name }}:{{ .User }}@{{ .Server }} "
        },
        {
          "type": "docker",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:docker.fg",
          "background": "p:docker.bg",
          "template": "  {{ .Context }} "
        },
        {
          "type": "kubectl",
          "style": "powerline",
          "powerline_symbol": "",
          "template": " \uDB84\uDCFE {{if .Namespace}}{{.Namespace}}{{else}}{{.Cluster}}/default{{end}} ",
          "foreground": "p:kubectl.fg",
          "background": "p:kubectl.bg"
        },
        {
          "type": "mvn",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:mvn.fg",
          "background": "p:mvn.bg",
          "template": " mvn \ue82c {{ .Full }} "
        },
        {
          "type": "npm",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:npm.fg",
          "background": "p:npm.bg"
        },
        {
          "type": "nx",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:nx.fg",
          "background": "p:nx.bg",
          "template": " NX {{ .Full }} ",
          "display_mode": "files"
        },
        {
          "type": "pnpm",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:pnpm.fg",
          "background": "p:pnpm.bg",
          "template": " 󰋁 {{ .Full }} ",
          "extensions": [
            ".pnpmfile.cjs",
            ".pnpmfile.js",
            "pnpm-lock.yaml",
            "pnpm-lock.json"
          ]
        },
        {
          "type": "gcp",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:gcp.fg",
          "background": "p:gcp.bg",
          "template": "  {{.Project}} :: {{.Account}} "
        },
        {
          "type": "java",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:java.fg",
          "background": "p:java.bg",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Major }}{{ end }}"
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "",
          "fetch_package_manager": true,
          "foreground": "p:node.fg",
          "background": "p:node.bg",
          "template": "  {{ .Full }}{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }} "
        },
        {
          "type": "php",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:php.fg",
          "background": "p:php.bg",
          "cache_duration": "24h",
          "display_mode": "files",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Major }}{{ end }}"
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "",
          "display_default": false,
          "foreground": "p:python.fg",
          "background": "p:python.bg",
          "template": " \uE73C {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Major }}{{.Minor}}{{ end }} "
        },
        {
          "type": "spotify",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:spotify.fg",
          "background": "p:spotify.bg",
          "template": "{{ if ne .Status \"stopped\" }}{{ .Icon }} {{ .Artist }}/{{ .Track }}{{ end }}"
        },
        {
          "type": "ytm",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:ytm.fg",
          "background": "p:ytm.bg",
          "template": "{{ if ne .Status \"stopped\" }}{{ .Icon }} {{ .Artist }}/{{ .Track }}{{ end }}",
          "cache": {
            "duration": "5s",
            "strategy": "session"
          }
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:execution-time.fg",
          "background": "p:execution-time.bg",
          "template": " <#fefefe></> {{ .FormattedMs }} ",
          "properties": {
            "threshold": 3000,
            "style": "austin",
            "always_enabled": false
          }
        },
        {
          "type": "os",
          "style": "powerline",
          "powerline_symbol": "\uE0B1",
          "foreground": "p:os.fg",
          "background": "p:os.bg",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "properties": {
            "macos": "mac"
          }
        }
      ]
    }
  ]
}